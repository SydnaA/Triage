<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <Button
        android:id="@+id/landscape_edit_personal_data_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:text="Edit Personal Data" />

    <Button
        android:id="@+id/landscape_view_symptoms_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="21dp"
        android:layout_toRightOf="@+id/landscape_edit_personal_data_button"
        android:text="View Symptoms" />

    <Button
        android:id="@+id/landscape_view_vitalsign_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/landscape_view_symptoms_button"
        android:layout_alignBottom="@+id/landscape_view_symptoms_button"
        android:layout_toLeftOf="@+id/landscape_edit_personal_data_button"
        android:text="View Vital Signs" />

    <TextView
        android:id="@+id/landscape_arrival_time_field"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/landscape_view_vitalsign_button"
        android:layout_below="@+id/textView3"
        android:layout_marginLeft="16dp"
        android:text="Medium Text"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/landscape_patient_name_field"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="22dp"
        android:text="Large Text"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/landscape_arrival_time_field"
        android:layout_alignLeft="@+id/landscape_view_symptoms_button"
        android:text="Time Seen By Doctor"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/landscape_arrival_time_field"
        android:layout_below="@+id/landscape_patient_name_field"
        android:text="Arrival Time"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/landscape_times_seen_by_doctor_field"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/landscape_arrival_time_field"
        android:layout_alignBottom="@+id/landscape_arrival_time_field"
        android:layout_alignRight="@+id/landscape_view_symptoms_button"
        android:text="Medium Text"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/landscape_urgency_level_field"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/landscape_view_vitalsign_button"
        android:layout_alignBottom="@+id/landscape_view_vitalsign_button"
        android:layout_alignLeft="@+id/landscape_edit_personal_data_button"
        android:layout_marginLeft="15dp"
        android:text="Large Text"
        android:textAppearance="?android:attr/textAppearanceLarge" />

</RelativeLayout>
